$re_suffix = qr/(?:$re_suffix)/i;
#print $re,"\n";

my $OVERWRITE = $ENV{OVERWRITE};
for (@ARGV) {
  my $old = $_;

  no warnings;
  my $old2 = $old;
  while (1) {
      s/^$re_prefix//; s/^\s+//;
      s/$re_suffix(\.\w+)?$/$1/;
      s/\s+(\.\w+)?$/$1/;
      last if $old2 eq $_;
      $old2 = $_;
  }
  use warnings;

  if ($old ne $_) {
    print "$old -> $_\n";
    if ((-e $_) && !$OVERWRITE) {
      warn "ERROR: $_ already exists, skipped\n";
    } else {
      rename $old, $_ or warn "ERROR: Can't rename $old -> $_: $!\n";
    }
  }
}
